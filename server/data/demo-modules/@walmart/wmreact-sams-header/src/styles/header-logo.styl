.header-Header {
  .header-Logo {
    logo-s-dimensions();

    margin: $xs-spacing auto;

    +breakpoint($breakpoint-m) {
      margin-left: $m-spacing;
      margin-right: $m-spacing;
    }

    +breakpoint($breakpoint-l) {
      logo-dimensions();

      margin: 0;
    }
  }

  .header-Logo-shippingPass {
    display: none;

    +breakpoint($breakpoint-l) {
      display: block;
    }
  }

  .is-fixed {
    .header-Logo {
      +breakpoint($breakpoint-l) {
        logo-s-dimensions();

        margin: $xs-spacing $m-spacing;
      }
    }

    .header-Logo-shippingPass {
      display: none;
    }
  }
}

.header-Header .header-Logo {
  background-image: url("http://m.samsclub.com/resources/images/logo-large.png");
  background-size: cover;
  height: 32px;
  width: 152px;
}

@media screen and (min-width: 64em) {
  .header-Header .is-fixed .header-Logo {
    height: 32px;
    width: 152px;
    margin: 8px 20px;
    margin-left: 0;
  }
}

@media (min-width: 48em) {
  .header-Header .header-Logo {
    margin-left: auto;
    margin-right: auto;
  }
}

.header-Header .header-Logo {
  +breakpoint($breakpoint-m-max, "max") {
    margin-left: 0;
  }
  margin: 4px auto;
}
