import { combineReducers } from "redux";
import { emailSignup } from "@walmart/wmreact-footer/lib/reducers/email-signup";
import { referenceId } from "@walmart/wmreact-footer/lib/reducers/reference-id";
import { quimbyDataReducer } from "@walmart/wmreact-footer/lib/tempo-core";

const footerReducer = combineReducers({
  emailSignup,
  referenceId,
  quimbyData: quimbyDataReducer
});

export default footerReducer;
