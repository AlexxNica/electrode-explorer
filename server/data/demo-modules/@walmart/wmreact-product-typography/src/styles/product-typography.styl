@require "~@walmart/wmreact-base/src/styles/base";
@require "~@walmart/wmreact-interactive/src/styles/interactive";
@require "~@walmart/wmreact-layout/src/styles/layout";
@require "~@walmart/wmreact-product-descriptors/src/styles/product-descriptors";
@require "./product-legal-badge";
@require "./product-secondary-info";

.SellerInfo {
  .SellerInfo-shipping-msg {
    font-size: 14px;
    margin-right: 5px;
  }

  .logo-return-container {
    line-height:75%;
  }

  .SellerInfo-logo {
    display: block;
    background-size: auto 20px;
    background-repeat: no-repeat;
    width: auto;
    height: 20px;
    margin: 8px 0;
  }

  .SellerInfo-walmart-logo {
    background-image: url("data:image/svg+xml;base64,");
  }

  .SellerInfo-return-policy {
    to-rem(font-size, 14px);

    vertical-align: middle;
  }

  .SellerInfo-Soi {
    to-rem(font-size, 12px);

    margin-left: $s-spacing;
    border-left: 1px solid $grey-light;
    padding-left: $s-spacing;
  }

  &.SellerInfo-responsive {
    .SellerInfo-Soi {
      +breakpoint($breakpoint-m) {
        display: none;
      }
    }
  }
}

mark {
  background-color: inherit;
  color: inherit;
  font-weight: 600;
}

.prod-PaddingRight--xs {
    padding-right: 8px;
}

.prod-PaddingLeft--xs {
   padding-left: 8px;
}
