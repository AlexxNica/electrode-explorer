@require "~@walmart/wmreact-base/src/styles/base";
@require "~@walmart/wmreact-layout/src/styles/layout";
@require "./button";
@require "./dropdown";
@require "./zoom";
@require "./paginator";

.btn-double button.btn {
  width: 38px;
  padding: 0;
}

.btn-double button.btn-fake-link {
  padding-left: .5rem;
}

.revealer-footer {
  position: relative;
  text-align: center;
  z-index: 9;

  &.is-active {
    margin-top: 20px;

    box-shadow: none;
  }

  &.border {
    border-top: 1px solid $grey-cool;
    box-shadow: 0 -18px 25px 0 rgba(254, 253, 253, .9);
  }

  .caret:after {
    transform-origin: 50%;
  }
}

