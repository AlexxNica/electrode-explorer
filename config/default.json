{
  "plugins": {
    "inert": {
      "enable": true
    },
    "./server/search-api": {},
    "./server/component-data": {},
    "./server/component-data/poll": {},
    "@walmart/electrode-react-webapp": {
      "options": {
        "pageTitle": "Electrode Explorer",
        "devServer": {
          "port": "2992"
        },
        "unbundledStyle": [
          "http://cdnjs.cloudflare.com/ajax/libs/codemirror/5.0.0/codemirror.min.css",
          "http://cdnjs.cloudflare.com/ajax/libs/codemirror/5.0.0/theme/monokai.min.css"
        ],
        "paths": {
          "/{args*}": {
            "view": "index",
            "content": {
              "module": "./view/index-view"
            }
          }
        },
        "cdn": {
          "enabled": false,
          "mapping": "config/assets.json"
        }
      }
    }
  },

  "githubApi": {
    "version": "3.0.0",
    "pathPrefix": "/api/v3",
    "protocol": "https",
    "host": "github.com"
  },

  "ui": {
    "basePath": "/explorer",
    "meta": {
      "name": "Electrode Showcase",
      "logo": "/explorer/img/electrode.png"
    },
    "github": {
      "protocol": "https",
      "basePath": "github.com"
    },
    "timeout": 10000
  },

  "ORGS": [

  ],

  "REPOS_USAGE_INCLUDE": [

  ],

  "REPOS_USAGE_EXCLUDE": [

  ],

  "MODULE_PREFIXES_INCLUDE": [

  ],

  "automaticUpdate": true,

  "NPM_WAITING_TIME": 300000,

  "POLL_INTERVAL": 1800000,

  "GHACCESS_TOKEN_NAME": "ECE_GHACCESS_TOKEN"
}
