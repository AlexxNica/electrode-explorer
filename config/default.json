{
  "services": {
    "consumerId": "91e1b78e-261b-4676-8317-4cf5bfa24a42",
    "privateKey": {
      "file": "config/private-key-qa.pem",
      "version": 2,
      "algorithm": "RSA",
      "encoding": "PKCS#8"
    }
  },
  "plugins": {
    "./server/search-api": {},
    "./server/component-data": {},
    "@walmart/electrode-react-webapp": {
      "options": {
        "pageTitle": "Electrode Portal",
        "devServer": {
          "port": "2992"
        },
        "unbundledStyle": [
          "http://cdnjs.cloudflare.com/ajax/libs/codemirror/5.0.0/codemirror.min.css",
          "http://cdnjs.cloudflare.com/ajax/libs/codemirror/5.0.0/theme/monokai.min.css"
        ],
        "paths": {
          "/{args*}": {
            "view": "index",
            "content": {
              "module": "./view/index-view"
            }
          }
        },
        "cdn": {
          "enabled": false,
          "mapping": "config/assets.json"
        }
      }
    }
  },

  "githubApi": {
    "version": "3.0.0",
    "pathPrefix": "/api/v3",
    "protocol": "https",
    "host": "gecgithub01.walmart.com"
  },

  "ui": {
    "basePath": "/portal",
    "meta": {
      "portalName": "Electrode Showcase",
      "portalLogo": "/portal/img/electrode.png"
    },
    "github": {
      "protocol": "https",
      "basePath": "gecgithub01.walmart.com"
    },
    "timeout": 10000
  },

  "REPOS_INCLUDE": [
    "electrode",
    "react",
    "redux",
    "R-"
  ],

  "REPOS_EXCLUDE": [
    "electrode-training",
    "electrode/portal",
    "electrode/electrode-showcase",
    "electrode/bootcamp-classes"
  ],

  "PREFIXES_INCLUDE": [
    "wmreact",
    "wmredux",
    "electrode"
  ],

  "automaticUpdate": true,

  "WAITING_TIME": 300000,

  "GHACCESS_TOKEN_NAME": "ECE_GHACCESS_TOKEN"
}
