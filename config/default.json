{
  "services": {
    "consumerId": "91e1b78e-261b-4676-8317-4cf5bfa24a42",
    "privateKey": {
      "file": "config/private-key-qa.pem",
      "version": 2,
      "algorithm": "RSA",
      "encoding": "PKCS#8"
    },
    "providers": {
      "@walmart/service-registry-client": {
      },
      "@walmart/iam2-auth-client": {
      },
      "@walmart/electrode-ccm-client": {
        "options": {
          "artifactId": "qa-service",
          "artifactVersion": "",
          "cloudEnvironment": "qa",
          "cloud": "",
          "cloudDc": "qa",
          "environment": "qa",
          "node": ""
        }
      },
      "@walmart/electrode-quimby-client": {}
    }
  },
  "ccm": {
    "autoLoad": true,
    "uiFilter": {
      "$..atlasXO": [
        "enable_guest.*",
        "responsive"
      ],
      "$.device.mobile.atlasP13n": [
        "irs.onehgConfig",
        "irs.maxTotal"
      ]
    },
    "keys": {
      "root": {
        "data": {
          "atlasXO": {
            "serviceName": "atlas-checkout",
            "+configNames": [
              "guest-checkout",
              "responsive-config",
              "opinion-lab"
            ]
          },
          "atlasApi": {
            "serviceName": "atlas-api",
            "+configNames": [
              "checkout"
            ]
          }
        }
      },
      "device": {
        "scopeTemplate": "/{tenantid}/{environment}/{cloudEnvironment}/{deviceType}/{node}",
        "profiles": {
          "default": {
            "tenantid": "01",
            "environment": "stg"
          },
          "mobile": {
            "deviceType": "mobile"
          },
          "desktop": {
            "deviceType": ""
          }
        },
        "data": {
          "atlasP13n": {
            "serviceName": "atlas-p13n",
            "+configNames": [
              "irsConfig"
            ]
          }
        }
      }
    }
  },
  "plugins": {
    "@walmart/store-info-plugin": {},
    "./server/component-data": {},
    "@walmart/electrode-react-webapp": {
      "options": {
        "pageTitle": "Electrode Portal",
        "devServer": {
          "port": "2992"
        },
        "unbundledJS": {
          "enterHead": [
            "{{readFile:config/perf.js}}",
            {
              "src": "https://beacon.qa.walmart.com/beacon.js"
            }
          ]
        },
        "paths": {
          "/{args*}": {
            "view": "index",
            "content": {
              "module": "./view/index-view"
            }
          }
        },
        "cdn": {
          "enabled": false,
          "mapping": "config/assets.json"
        }
      }
    },
    "@walmart/auth-plugin": {
      "priority": 20
    }
  },
  "ui": {
    "basePath": "/portal",
    "automaticUpdate": true,
    "meta": {
      "portalName": "Electrode Showcase"
    }
  }
}
