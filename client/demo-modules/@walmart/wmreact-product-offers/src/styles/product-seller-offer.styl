.prod-SellerOffer {

  .logo-inline {
    display: inline-block;
  }

  .seller-offer-sort-active {
    padding: 2px;
    border-radius: 4px;
    background-color: $blue-light;
    margin-right: $xs-spacing;
  }

  .product-shipping-price {
    position: relative;
    top: -2px;
    padding-left: 3px;
  }

  .seller-offer-sm-price {
    to-rem(font-size, 20px);

    color: $blue;
  }

  .seller-offer-l-price {
    to-rem(line-height, 23px);

    .Price-characteristic {
      font-weight: 600;
    }
  }

  .Price-submap {
    .Price-old-text {
      to-rem(font-size, 20px);
      padding-bottom: 10px;
    }

    .Price-submapFlyout-content {
      line-height: 20px;
    }

    .wmicon-help {
      position: relative;
      top: -3px;
    }

    .slidepanel-header {
      background-color: $blue-light;
    }
  }

  .seller-offer-shipping-msg {
    to-rem(font-size, 14px);
  }

  .shipping-pass-msg {
    to-rem(font-size, 14px);
  }

  .seller-offer-shipping-pass-logo {
    background-image: url("data:image/svg+xml;base64,");
    width: 78px;
    height: 13px;
  }

  &.responsive-seller-offer {
    .product-shipping-price {
      +breakpoint($breakpoint-m) {
        padding-left: 0;
        top: 4px;
      }
    }

    .seller-offer-sort-active {
      +breakpoint($breakpoint-m) {
        padding: 0;
        border: 0;
        background: none;
      }
    }

    .seller-offer-shipping-msg {
      +breakpoint($breakpoint-m) {
        to-rem(font-size, 16px);
      }
    }
    
    .seller-offer-shipping-pass-logo {
      +breakpoint($breakpoint-m) {
        position: relative;
        top: 3px;
        left: 5px;
      }
    }

    .Price-submap {
      .Price-old-text {
        +breakpoint($breakpoint-m) {
          to-rem(font-size, 25px);
        }
      }
      .help-text-trigger {
        margin-left: 6px;
      }
    }
  }

}
