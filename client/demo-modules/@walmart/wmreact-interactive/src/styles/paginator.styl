// @from @walmart/hydra/src/common/base/paginator.styl

/*!
Paginator Button
*/

$paginator-btn-color ?= {
  standard: {
    text: #333,
    text-hover: #555,
    bg: #fff,
    bg-hover: #ccc
    bg-active: #bbb
    border: #aaa
  },
  alt: {
    text: #333,
    bg: #ccc // @stylint ignore
  }
}

$paginator-btn-height = 40px;

.paginator-btn {
  position: relative;

  display: inline-block;
  width: $paginator-btn-height;
  height: @width;
  padding: 0;
  border: 1px solid $paginator-btn-color.standard.border;
  border-radius: 50%;

  background: $paginator-btn-color.standard.bg;
  color: $paginator-btn-color.standard.text;
  font-size: 16px;
  text-align: center;
  vertical-align: middle;
  user-select: none;

  &:before {
    +ifWalmarticons() {
      walmarticons();
    }

    position: relative;
    bottom: 1px;

    font-size: 15px;
    line-height: $paginator-btn-height;
  }

  &:hover,
  &:focus {
    border-color: $paginator-btn-color.standard.bg-hover;

    background: @border-color;
    color: $paginator-btn-color.standard.text-hover;
    outline: none;
  }

  &:active {
    border-color: $paginator-btn-color.standard.bg-active;

    background: @border-color;
    color: $paginator-btn-color.standard.text-hover;
  }

  &.hidden {
    opacity: 0;
  }
}

.js-arrow-hidden {
  visibility: hidden;
}

$tenant-paginator-btn-alt();

.paginator-btn-prev {
  left: 0;

  &:before {
    +ifWalmarticons() {
      wmicon("angle-left");
    }

    right: 1px;
  }
}

.paginator-btn-next {
  right: 0;

  &:before {
    +ifWalmarticons() {
      wmicon("angle-right");
    }

    left: 1px;
  }
}

/*!
Paginator Hairline Button

For use in carousels. Comes in three color schemes and two sizes. For typical
carousel usage, include the `.paginator-hairline-btn-positioned` modifier
class.

.paginator-hairline-btn-light - Light version.
.paginator-hairline-btn-dark - Dark version.
.paginator-hairline-btn-large - Large version.
.paginator-hairline-btn-large.paginator-hairline-btn-light - Large light version.
.paginator-hairline-btn-large.paginator-hairline-btn-dark - Large dark version.
.paginator-hairline-btn-light-no-hover - Light version without hover state.
.paginator-hairline-btn-dark-no-hover - Dark version without hover state.
.paginator-hairline-btn-large..paginator-hairline-btn-light-no-hover - Large light version without hover state.
.paginator-hairline-btn-large..paginator-hairline-btn-dark-no-hover - Large dark version without hover state.

Styleguide 12.2
*/

.paginator-hairline-btn {
  display: inline-block;
  width: 40px;
  height: @width;
  padding: 0;
  border: 0;

  background-color: transparent;
  background-position: center;
  outline: none;
  wmzindex("paginator-hairline-btn");
}

.paginator-hairline-btn-large {
  width: 45px;
  height: 68px;
}

.paginator-hairline-btn-full-height {
  height: auto;
}

.paginator-hairline-btn-full-width {
  width: auto;
}

/*!
Paginator Hairline Button Positioning

The `.paginator-hairline-btn-positioned` modifier class pins hairline buttons
to the edge of their parent, for use when buttons should be displayed on top
of a carousel.

For use with `prev` and `next` directional modifiers, the
`.paginator-hairline-btn-voffset` modifier offsets directional buttons by
30px from the bottom of the container. This allows them to remain vertically
centered over carousel content when carousels include statically positioned
paginators below them.

Styleguide 12.2.1
*/

// By default, vertically and horizontally centers buttons. Each directional
// modifier class adds a value of `auto` to the direction opposite of the one
// that it is anchored to, pulling it to the appropriate side.
.paginator-hairline-btn-positioned {
  absolute-center();

  &.paginator-hairline-btn-prev {
    right: auto;
  }

  &.paginator-hairline-btn-next {
    left: auto;
  }

  &.paginator-hairline-btn-up {
    bottom: auto;
  }

  &.paginator-hairline-btn-down {
    top: auto;
  }
}

.paginator-hairline-btn-voffset {
  bottom: 30px;
}

.paginator-hairline-btn-voffset-large {
  bottom: 34px;
}

/*!
Paginator List

Styleguide 12.3
*/

$paginator-list-color ?= {
  text: #333,
  border-hover: #ccc
}

.paginator-list {
  display: inline-block;
  margin: 0;
  padding: 0;

  list-style: none;
  vertical-align: middle;

  li {
    display: inline-block;
    margin-left: 8px;

    font-size: 16px;
    vertical-align: middle;

    &:first-child {
      margin-left: 0;
    }
  }

  .paginator-list-gap {
    &:before {
      content: "\2026";
      position: relative;
      bottom: 1px;

      color: $paginator-list-color.text;
      font-size: 17px;
    }
  }

  a,
  button {
    display: block;
    // To ensure identical rendering on `a` and `button`, we use padding and line-height instead of
    // height for these elements.
    padding: 2px 7px 4px;
    border: 1px solid transparent;

    background: transparent;
    color: $paginator-list-color.text;
    line-height: 1;
    text-align: center;

    &:hover,
    &:focus {
      border-color: $paginator-list-color.border-hover;

      color: $paginator-list-color.text;
      outline: none;
    }
  }
}

$tenant-paginator-list();

/*!
Paginator

Styleguide 12.4
*/

.paginator {
  // Remove space between inline-block elements
  font-size: 0;
  text-align: center;

  .paginator-list {
    // Paginator lists used in a paginator element with prev/next buttons have margin on both sides
    // to separate them from the buttons.
    margin: 0 18px;
  }
}

/*!
Carousel Paginator

.carousel-paginator-list-mini - Mini variant.
.carousel-paginator-list-positioned - Absolute positioned variant.

Styleguide 12.5
*/

$carousel-paginator-color ?= {
  standard: "#ccc",
  hover: "#bbb",
  active: "#000"
}

$carousel-paginator-list {
  display: block;
  margin: 0;
  padding: 20px 0 0;

  font-size: 0;
  list-style: none;
  text-align: center;
  user-select: none;

  li {
    display: inline-block;
    margin-left: 18px;

    vertical-align: top;

    &:first-child {
      margin-left: 0;
    }
  }
}

carousel-paginator-item-mini() {
  width: 10px;
  height: @width;
}

carousel-paginator-item-active() {
  background: $carousel-paginator-color.active;
  cursor: default;
}

$carousel-paginator-item {
  ir();

  display: block;
  width: 14px;
  height: @width;
  padding: 0;
  border: 0;
  border-radius: 50%;

  background: $carousel-paginator-color.standard;
  color: transparent;

  &:hover,
  &:focus {
    background: $carousel-paginator-color.hover;
    outline: none;
  }

  &.active {
    carousel-paginator-item-active();
  }
}

.carousel-paginator-list {
  @extend $carousel-paginator-list;
}

.carousel-one-up {
  .carousel-paginator-list {
    bottom: 0;
    left: 0;
    margin: 0 auto;
    padding: 20px 0;
    position: absolute;
    right: 0;
    width: 150px;
  }
}

.carousel-one-up-responsive {
  .carousel-paginator-list {
    margin: 0 auto;
    padding: $xs-spacing 0;

    +breakpoint($breakpoint-m) {
      padding-top: $s-spacing;
      padding-bottom: $s-spacing;
    }

    .carousel-paginator-item {
      height: 7px;
      width: 7px;

      +breakpoint($breakpoint-m) {
        height: 10px;
        width: 10px;
      }
    }

    li {
      margin-left: $xs-spacing;

      +breakpoint($breakpoint-m) {
        margin-left: 16px;
      }

      &:first-child {
        margin-left: 0;
      }
    }
  }
}

.carousel-paginator-list-positioned {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
}

.carousel-paginator-item {
  @extend $carousel-paginator-item;

  .slick-active > & {
    carousel-paginator-item-active();
  }

  .carousel-paginator-list-mini & {
    carousel-paginator-item-mini();
  }
}

// Deprecated, use `.carousel-paginator-list` and `.carousel-paginator-item`
// components instead of `.carousel-paginator`.
.carousel-paginator {
  position: relative;

  height: 55px;

  .paginator-btn {
    position: absolute;
    top: 50%;

    margin-top: -($paginator-btn-height/2);
  }

  .paginator-list {
    @extend $carousel-paginator-list;

    a, button {
      @extend $carousel-paginator-item;
    }
  }
}

.carousel-paginator-mini .paginator-list {
  a, button {
    carousel-paginator-item-mini();
  }
}

