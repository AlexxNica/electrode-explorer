/* ==========================================================================
   Carousel
   ========================================================================== */

.carousel-wrapper {
  position: relative;

  overflow: hidden;
  width: 100%;
  height: 100%;
}

.carousel {
  wmzindex("none");

  position: relative;

  overflow: hidden;

  -webkit-transform: translate3d(0, 0, 0);
  -webkit-backface-visibility: hidden;
  -webkit-perspective: 1000;
  transform: translate3d(0, 0, 0);

  text-align: center;
  margin: 0 auto;

  .items {
    position: relative;

    display: block;
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
  }

  .items > li {
    wmzindex("none");

    position: absolute;

    display: block;
    height: 100%;

    color: rgba(0, 0, 0, .8);

    button, a, img, area {
      -webkit-tap-highlight-color: rgba(0, 0, 0, .3);
    }

    .flyout-modal {
      margin-left: -75px;
      margin-bottom: 2px;
      padding: 6px;

      font-size: 14px;
    }
  }

  .items > .selected {
    wmzindex("basic");

    left: 0;
  }

  .progress {
    wmzindex("basic-increment");

    position: absolute;
    top: 0;
    left: 0;

    width: 0;
    height: 4px;

    background-color: rgba(255, 255, 255, .5);
    transition: width 500ms linear;
  }

  a {
    cursor: pointer;
  }

  a:focus {
    outline: none;
  }

  .next,
  .previous,
  .up,
  .down {
    wmzindex("basic");

    position: absolute;

    display: block;
    border: none;

    background: rgba(255, 255, 255, 0);
    color: $blue;
    font-size: 125px;
    font-weight: lighter;
    line-height: 40px;
    text-align: center;
    transition: background-color 250ms ease-in, color 100ms ease-in;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -o-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;

    &:hover {
      color: $white;
    }
  }

  .next,
  .previous {
    top: 50%;
    width: 44px;
    height: 70px;

    margin-top: -(@height/2);
  }

  .next {
    right: 0;
  }

  .previous {
    left: 0;
  }

  .up {
    top: 0;
  }

  .down {
    bottom: 0;
  }

  .paginator-btn {
    wmzindex("carousel-paginator");

    position: absolute;
    top: 50%;

    margin-top: -($paginator-btn-height/2);
  }

  .carousel-paginator,
  .chapter-paginator {
    wmzindex("carousel-paginator");

    position: absolute;
    right: 0;
    bottom: 0;
    left: 0;
  }

  .chapter-paginator {
    height: 95px;
  }

  // TODO: Remove these magic number-based legacy styles.
  .paginator-btn-next {
    right: 20px;
  }

  .paginator-btn-prev {
    left: 5px;
  }
}

.carousel-touch {
  .touch-wrap {
    overflow-x: scroll;
  }

  .items {
    > li {
      position: relative;
      float: left;
    }
  }
}

.image-map-swipe {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.carousel.slide .animate {
  transition: left 500ms ease-in-out;
}

.carousel.slide .animate-vertical {
  transition: top 500ms ease-in-out;
}

.carousel-loading {
  .items {
    white-space: nowrap;
    overflow: hidden;

    .carousel-n-up-responsive .carousel-paginator-list {
      height: 50px;
    }

    .tile-heading span, .tile .tile-heading, .Tile-heading {
      white-space: normal;
    }

    > li {
      wmzindex("none");

      position: static;
      display: inline-block;
      height: 100%;

      color: rgba(0, 0, 0, .8);
      white-space: normal;
    }
  }
}

.pov-carousel-loading {
  .items > li {
    wmzindex("none");

    position: static;

    display: none;
    height: 100%;

    color: rgba(0, 0, 0, .8);
    background-color: $white;
  }

  .items > li:first-child {
    display: inline-block;
    width: 100%;
    height: 100%;
  }
}

.carousel-n-up {
  text-align: left;
  padding: 0;

  .items {
    .visible .tile:after {
      opacity: 0;
      content: none;
    }

    // Put gradient on half and hidden tiles
    .tile:after {
      content: "";
      position: absolute;
      opacity: 1;
      top: 0;
      right: 0;

      display: block;
      width: 100%;
      height: 100%;

      // Fallback IE9 non-gradient
      background: rgba(255, 255, 255, .7);
      // IE10+ gradient (for smoothing the visual on sliding)
      background: linear-gradient(right, rgba(255, 255, 255, .7), rgba(255, 255, 255, .2));
      transition: .3s ease opacity, .3s ease visibility;
      visibility: visible;
    }

    > li {
      height: auto;
      background: transparent;
      vertical-align: top;
    }
  }

  .paginator-btn {
    opacity: 0;
    transition: .3s ease opacity;
  }

  &:hover {
    .paginator-btn {
      opacity: 1;

      &.hidden {
        opacity: 0;
      }
    }
  }
}

