  box-shadow: 0 0 1px rgba(0, 0, 0, .2), 1px 1px 3px rgba(0, 0, 0, .25);
  // Remove inline-block space.
  font-size: 0;
  overflow: hidden;

  li {
    display: inline-block;
    height: 100%;

    font-size: 12px;
    vertical-align: top;

    &:first-child {
      .chapter-paginator-item {
        padding-left: 20px;

        &.chapter-paginator-item-thumb {
          padding-left: 20px;
        }
      }
    }

    &.last {
